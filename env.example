# Dados do ADMIN inicial
ADMIN_NOME_COMPLETO=  # Nome completo do admin inicial
ADMIN_EMAIL=          # E-mail do admin inicial
ADMIN_SENHA=          # Senha do admin inicial
ADMIN_CPF=            # CPF do admin inicial

# Configurações do JWT
JWT_SECRET_KEY=       # Chave secreta para geração dos tokens JWT
JWT_ALGORITHM=        # Algoritmo do JWT (ex: HS256)
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=  # Tempo de expiração do token JWT (em minutos)

# Configurações de Cookies (HttpOnly e SameSite)
COOKIE_NAME=          # Nome do cookie de autenticação (padrão: access_token)
COOKIE_HTTPONLY=      # Cookie HttpOnly (padrão: true)
COOKIE_SAMESITE=      # Política SameSite (padrão: lax)
COOKIE_SECURE=        # Cookie seguro (padrão: false, true em produção com HTTPS)
COOKIE_DOMAIN=        # Domínio do cookie (padrão: None para localhost)

# Configurações do Banco de Dados
DATABASE_URL=         # URL de conexão com o banco de dados
POSTGRES_USER=        # Usuário do banco de dados PostgreSQL
POSTGRES_PASSWORD=    # Senha do banco de dados PostgreSQL
POSTGRES_DB=          # Nome do banco de dados PostgreSQL
POSTGRES_HOST=        # Host do banco de dados PostgreSQL
POSTGRES_PORT=        # Porta do banco de dados PostgreSQL

# Configurações da API
API_HOST=             # Host da API
API_PORT=             # Porta da API

# Configurações do NextCloud (WebDAV)
NEXTCLOUD_BASE_URL=   # URL base do servidor NextCloud (ex: https://cloud.example.com)
NEXTCLOUD_USERNAME=   # Username para autenticação WebDAV
NEXTCLOUD_PASSWORD=   # Senha ou App Password para autenticação WebDAV
NEXTCLOUD_WEBDAV_PATH= # Path base do WebDAV (padrão: /remote.php/dav)
NEXTCLOUD_USER_PATH=  # Path do usuário dentro do WebDAV (ex: /files/username)
NEXTCLOUD_MAX_PAGE_SIZE= # Tamanho máximo de página para paginação (padrão: 100)
NEXTCLOUD_VERIFY_SSL= # Verificar certificado SSL (true/false, padrão: true. Use false apenas para desenvolvimento)

# Configurações de Sincronização NextCloud
NEXTCLOUD_SYNC_ACTIVITY_API_INTERVAL=5  # Intervalo em minutos para sincronização via Activity API (padrão: 5 minutos)
NEXTCLOUD_SYNC_WEBDAV_INTERVAL=300  # Intervalo em minutos para sincronização via WebDAV (padrão: 300 = 5 horas)
NEXTCLOUD_SYNC_INITIAL_ON_STARTUP=true  # Executar sincronização completa ao iniciar o sistema (true/false, padrão: true)
NEXTCLOUD_SYNC_MAX_RETRIES=3  # Número máximo de tentativas em caso de erro (padrão: 3)
NEXTCLOUD_SYNC_RETRY_DELAY=30  # Delay em segundos entre tentativas de retry (padrão: 30 segundos)
NEXTCLOUD_SYNC_BATCH_SIZE=50  # Tamanho do lote para processamento de imagens (padrão: 50 imagens por lote)

# Configurações de Timezone
TIMEZONE=America/Sao_Paulo  # Fuso horário padrão (Brasília), pode ser alterado (ex: "UTC", "America/New_York")
